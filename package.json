{
  "version": "1.4.1",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/flexper/rosetty-react.git"
  },
  "bugs": {
    "url": "https://github.com/flexper/rosetty-react/issues",
    "email": "contact@flexper.fr"
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "eslint src test",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "rosetty-react",
  "author": "Flexper",
  "module": "dist/rosetty-react.esm.js",
  "size-limit": [
    {
      "path": "dist/rosetty-react.cjs.production.min.js",
      "limit": "300 KB"
    },
    {
      "path": "dist/rosetty-react.esm.js",
      "limit": "200 KB"
    }
  ],
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "lcov"
    ]
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "8.1.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@types/react": "18.0.21",
    "@typescript-eslint/eslint-plugin": "5.39.0",
    "@typescript-eslint/parser": "5.39.0",
    "eslint": "8.24.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "husky": "8.0.1",
    "jest": "29.1.2",
    "np": "7.6.2",
    "prettier": "2.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "size-limit": "8.1.0",
    "tsdx": "0.14.1",
    "tslib": "2.4.0",
    "typescript": "4.8.4"
  },
  "dependencies": {
    "rosetty": "^1.4.0"
  }
}
